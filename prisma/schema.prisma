generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id             String  @id @default(auto()) @map("_id") @db.ObjectId
  fullname       String
  email          String
  password       String
  isTwoFaEnabled Boolean @default(false)

  @@unique([email])
}
